set nocompatible
syntax on
set encoding=utf-8
set fileencodings=ucs-bom,utf-8,cp936,gb18030,big5,euc-jp,euc-kr,latin1
set termencoding=utf-8
set formatoptions+=m
set formatoptions+=B
set tabstop=4
set softtabstop=4
set shiftwidth=4
set smarttab
set number
"set nonumber
"inoremap ( ()<ESC>i
"inoremap [ []<ESC>i
"inoremap { {}<ESC>i
